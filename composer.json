{
  "require": {
    "clubstudioltd/craft-inline-svg": "2.0.0",
    "craftcms/ckeditor": "3.6.0",
    "craftcms/cms": "4.5.13",
    "hybridinteractive/craft-position-fieldtype": "4.0.0",
    "nystudio107/craft-minify": "4.0.0-beta.2",
    "nystudio107/craft-vite": "4.0.7",
    "spacecatninja/imager-x": "4.2.3",
    "verbb/super-table": "3.0.12",
    "verbb/tablemaker": "4.0.7",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.0",
    "yiisoft/yii2-shell": "^2.0.3",
    "cweagans/composer-patches": "dev-main#a3884aeb0173e24b6ab9330c7fe28ebff114c141"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true,
      "cweagans/composer-patches": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    },
    {
      "type": "vcs",
      "url": "https://github.com/cweagans/composer-patches.git"
    }
  ]
}
